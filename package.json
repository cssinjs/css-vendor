{
  "name": "css-vendor",
  "description": "CSS vendor prefix detection and property feature testing.",
  "version": "1.1.0",
  "author": {
    "name": "Oleg Slobodskoi",
    "email": "oleg008@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cssinjs/css-vendor.git"
  },
  "keywords": [
    "css",
    "vendor",
    "feature",
    "test",
    "prefix",
    "cssinjs",
    "jss",
    "css-in-js"
  ],
  "engines": {},
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "build": "npm run clean && npm run build:lib && npm run build:dist",
    "build:lib": "babel src --out-dir lib",
    "build:dist": "npm run build:dist:max && npm run build:dist:min",
    "build:dist:max": "cross-env webpack --mode=development src/index.js --output-filename=css-vendor.js",
    "build:dist:min": "cross-env webpack --mode=production src/index.js --output-filename=css-vendor.min.js",
    "clean": "rimraf {lib,dist,tmp}/*",
    "lint": "eslint ./src ./tests --fix",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test karma start --single-run ",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "bench": "cross-env NODE_ENV=benchmark karma start --single-run",
    "prepublishOnly": "npm run all"
  },
  "license": "MIT",
  "main": "./lib/index",
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "caniuse-support": "^0.4.2",
    "cross-env": "^5.2.0",
    "es5-shim": "^4.5.10",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-jss": "^3.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "expect.js": "^0.3.1",
    "karma": "^2.0.4",
    "karma-benchmark": "^0.7.1",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-browserstack-launcher": "^1.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^3.0.0",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "postcss-js": "^1.0.1",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "is-in-browser": "^1.0.2"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "files": [
    "dist",
    "lib"
  ]
}
